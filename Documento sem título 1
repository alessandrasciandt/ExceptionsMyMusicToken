// Curso Udemy
https://www.udemy.com/course/bancos-de-dados-relacionais-basico-avancado/

 void test_buscarMusica() throws Exception {
        Music music = new Music();
        music.setId("4ffb5d4f-8b7f-4996-b84b-ecf751f52eea");
        music.setName("B.B. On Mars");

        Artist artist = new Artist();
        artist.setId("30ab1678-c616-4314-adcc-918aff5a7a13");
        artist.setName("Alice Cooper");
        music.setArtist(artist);

        given(musicService.findByArtistOrMusic("B.B. On Mars")).willReturn(new ArrayList<>());

        RequestBuilder request = get("/musicas?filtro=B.B. On Mars");
        MvcResult result = mvc.perform(request).andReturn();
        assertEquals(
                "[{\"id\":\"4ffb5d4f-8b7f-4996-b84b-ecf751f52eea\",\"name\":\"B.B. On Mars\",\"artist\":{\"id\":\"30ab1678-c616-4314-adcc-918aff5a7a13\",\"name\":\"Alice Cooper\"}}]",
                result.getResponse().getContentAsString());
    }



